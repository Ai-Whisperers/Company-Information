# Company-Information Org OS Environment Variables
# Copy to .env and fill in your actual values
# NEVER commit .env to git - it's in .gitignore

# GitHub Configuration
GITHUB_PAT=your_github_personal_access_token_here
GITHUB_TOKEN=your_github_personal_access_token_here
GITHUB_ORG=Ai-Whisperers

# Azure DevOps Configuration
AZURE_DEVOPS_PAT=your_azure_devops_personal_access_token_here
AZURE_DEVOPS_ORG=aiwhisperer
AZURE_DEVOPS_PROJECT=Business Setup
AZURE_DEVOPS_BASE_URL=https://dev.azure.com/aiwhisperer

# Database Configuration
POSTGRES_CONNECTION_STRING=postgresql://user:password@localhost:5432/database
DATABASE_URL=postgresql://user:password@localhost:5432/orgos_db

# Redis Configuration (for job queues)
REDIS_HOST=localhost
REDIS_PORT=6379

# Port Configuration
DASHBOARD_PORT=3001
JOBS_PORT=4000

# Dashboard Configuration
DASHBOARD_URL=http://localhost:3001
NEXT_PUBLIC_API_URL=http://localhost:4000

# Jobs Service Configuration
JOBS_SERVICE_URL=http://localhost:4000
JOBS_API_KEY=your_internal_api_key

# File System Access
# Set to your project root directory (will be auto-detected by PathResolver in scripts)
# For manual override, use absolute path to AI-Whisperers parent directory
FILESYSTEM_ROOT=/path/to/your/AI-Whisperers
FILESYSTEM_ALLOW_WRITE=true

# Authentication
NEXTAUTH_URL=http://localhost:3001
NEXTAUTH_SECRET=your_nextauth_secret_key_min_32_chars
GITHUB_CLIENT_ID=your_github_oauth_app_client_id
GITHUB_CLIENT_SECRET=your_github_oauth_app_client_secret

# Notification Services
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/WEBHOOK/URL
TEAMS_WEBHOOK_URL=https://outlook.office.com/webhook/YOUR/WEBHOOK/URL

# Feature Flags
ENABLE_ADO_SYNC=true
ENABLE_SLACK_NOTIFICATIONS=true
ENABLE_AUTO_REPAIR=false

# Logging
LOG_LEVEL=info
NODE_ENV=development