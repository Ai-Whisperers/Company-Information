{
  "name": "@ai-whisperers/orchestration",
  "version": "1.0.0",
  "description": "Multi-agent orchestration system for AI-Whisperers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist coverage",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern='core/__tests__'",
    "test:integration": "jest --testPathPattern='workflows/__tests__'",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "workflow:org-pulse": "ts-node orchestration/workflows/weekly-org-pulse.ts",
    "workflow:compliance": "ts-node orchestration/workflows/compliance-audit.ts"
  },
  "keywords": [
    "orchestration",
    "multi-agent",
    "automation",
    "workflows"
  ],
  "author": "AI-Whisperers",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/jest": "^29.5.0",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "ts-jest": "^29.1.0",
    "jest": "^29.7.0"
  },
  "dependencies": {},
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0"
  },
  "files": [
    "dist",
    "orchestration/config/*.json",
    "README.md",
    "MCP_EVOLUTION_ROADMAP.md",
    "INTEGRATION_GUIDE.md"
  ]
}
